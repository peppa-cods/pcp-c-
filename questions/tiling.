#include <iostream>
#include <string>

using namespace std;
int main(){
	int n;
	cin >> n;
	int t1 = 1;
	int t2 = 1;
	int t3 = 0;
	if (n == 1){
		t3 = 1;
		} else if (n == 2){
			t3 = 3;
		} else {
			for (int i = 2; i<=n; i++){
				t3 = ((2*t1) + t2) % 12345;
				t1 = t2;
				t2 = t3;
}
}
printf("%d", t3);
}
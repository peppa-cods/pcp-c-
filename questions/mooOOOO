#include <iostream>
#include <string>

using namespace std;
int main(){
	int n;
	cin >> n;
	string t1,t2;
	t1 = "moo";
	if (1<=n){
		for (int k=1; k<=n; k++){
			string os = "";
			for (int i=0; i<k+2; i++){
				os += "o";
			}
			t2 = t1 +"m" + os + t1;
			if (t2.length() > n){break;
				} else{
					t1 = t2;
					t2="";
					}

	}
		}else{
			return 0;
		}
		cout << t2[n-1] <<endl;
}
#include <iostream>
#include <string>
  
using namespace std;
int main(){
    int counter = 0;
    int n;
    cin >> n;
    long a=1;
    long b=2;
    long c=1;
		if(n == 1) {
			c = 1;
		} else if(n == 2){
			c =  2;
		}else{
			for(int i=3; i<=n; i++){
        c=(a+b);
        a=b;
        b=c;
    }

		}
    cout << c;
}